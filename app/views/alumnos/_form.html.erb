<%= simple_form_for(@alumno) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :nacionalidad %>
    <%= f.input :cedula %>
    <%= f.input :nombre %>
    <%= f.input :apellido %>
    <%= f.input :esta_activo %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
