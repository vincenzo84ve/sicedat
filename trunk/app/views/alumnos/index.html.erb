<h1>Listing alumnos</h1>

<table>
  <tr>
    <th>Nacionalidad</th>
    <th>Cedula</th>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Correo</th>
    <th>Esta activo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alumnos.each do |alumno| %>
  <tr>
    <td><%= alumno.nacionalidad %></td>
    <td><%= alumno.cedula %></td>
    <td><%= alumno.nombre %></td>
    <td><%= alumno.apellido %></td>
    <td><%= alumno.correo %></td>
    <td><%= alumno.esta_activo %></td>
    <td><%= link_to 'Show', alumno %></td>
    <td><%= link_to 'Edit', edit_alumno_path(alumno) %></td>
    <td><%= link_to 'Destroy', alumno, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Alumno', new_alumno_path %>
